{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    // "jsx": "react", // OR "react"
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "custom.d.ts"
  ]
}